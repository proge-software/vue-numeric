{
  "name": "vue-numeric",
  "version": "2.3.0",
  "description": "Input field component to display currency value based on Vue.",
  "author": "Kevin Ongko",
  "scripts": {
    "build": "webpack --config webpack.prod.js --progress",
    "buildDev": "webpack --config webpack.dev.js --progress",
